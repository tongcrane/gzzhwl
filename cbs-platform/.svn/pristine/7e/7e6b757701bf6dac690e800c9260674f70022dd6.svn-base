.skin-tb{-webkit-user-select:none;-moz-user-select:none;-o-user-select:none;user-select:none;font-size:12px;color:#999;}

.skin-tb a{color:#36c;cursor:pointer;font-size:12px;color:#333;}
.skin-tb a:hover{border:1px solid #fd6d01;}


.skin-tb .pagination-start,.skin-tb .pagination-prev,.skin-tb .pagination-next,.skin-tb .pagination-end{width:36px;background:url(http://img04.taobaocdn.com/tps/i4/T1cP85XihuXXXXXXXX-13-101.png) no-repeat;}
.skin-tb .pagination-start,.skin-tb .pagination-prev,.skin-tb .pagination-next,.skin-tb .pagination-end,.skin-tb .pagination-spec,.skin-tb .pagination-curr,.skin-tb .pagination-break{min-width:28px;_width:28px;border:1px solid #d2d2d2;display:inline;float:left;height:24px;margin-right:12px;padding:0 5px;line-height:24px;text-align:center;vertical-align:top;white-space:nowrap;overflow:hidden;text-decoration:none;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
.skin-tb .pagination-start{padding-left:16px;background-position:10px 8px;font-size:12px;color:#d2d2d2;line-height:22px;width:78px;}
.skin-tb .pagination-prev{padding-left:16px;background-position:6px -12px;line-height:22px;width:78px;color:#333;}
.skin-tb .pagination-curr{font-weight:700;color:#fff;background-color:#ff6037;border:1px solid #ff6037;font-size:12px;}
.skin-tb .pagination-break{color:#c0c0c0;border:0;}
.skin-tb .pagination-next{padding-right:16px;background-position:52px -50px;color:#333;font-size:12px;width:78px;color:#333;}
.skin-tb .pagination-end{padding-right:16px;background-position:58px -31px;width:78px;color:#d2d2d2;font-size:12px;}

.skin-tb .pagination-jump{width:52px;height:24px;line-height:24px;text-align:center;font-size:12px;color:#fff;background-color:#ff6037;margin-left:12px;-o-transition:background .5s;-ms-transition:background .5s;-moz-transition:background .5s;-webkit-transition:background .5s;}
.skin-tb .pagination-jump:hover{background-color:#ff4814;-o-transition:background .5s;-ms-transition:background .5s;-moz-transition:background .5s;-webkit-transition:background .5s;}

.skin-tb .pagination-goto{width:28px;height:22px;border:1px solid #c8c8c8;text-align:center;line-height:22px;color:#202028;font-size:12px;margin:0 8px;}
