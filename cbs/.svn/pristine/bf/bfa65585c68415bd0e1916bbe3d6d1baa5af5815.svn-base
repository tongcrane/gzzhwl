
#header{position: absolute;top:0;left:0;z-index: 99;}
#container{width:1200px;height:100%;margin:0 auto;padding-top:60px;position: relative;background: #eee; box-sizing: border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;}
#map{height:100%;}



.map_search{height:45px;width:410px;position: absolute;left:20px;top:92px;border-radius:4px;z-index: 999;background:#fff;}
.search_box,.search_btn{float:left;height:45px;}
.search_btn{float:right;}
.search_btn{width:64px;border-top-right-radius: 4px;border-bottom-right-radius: 4px;background:#e65e20 url(../img/search-icon-big.png) no-repeat center;}
.search_box .selec_t{width:96px;height:45px;margin-top:0;vertical-align: top;color:#333;font-size:16px;border:none;padding-left:10px;}
.search_box .inpu_t{font-size:16px;color:#333;height:45px;line-height: 45px;width:212px;padding-left:10px;margin-left:4px;}
.map_search:before{content: '';display: block;height:35px;border-left: 1px solid #e0e0e0;position:absolute;top:5px;left:113px;}

/* 提示信息 */
.map_tips{}
.tip_con>span,.tip_con>p{display: inline-block;font-size:15px;}
.tip_con>span{width:80px;height:26px;line-height: 26px;text-align:center;}
.tip_con>p{padding-right:10px;}

/* 下拉框样式 */
.ui-multiselect{padding:4px 0 5px 10px !important;}
.selectDark.ui-multiselect.ui-widget {background:#fff;}
.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default{border:none !important;outline: none;}
button.selectDark span,.ui-multiselect-option>span{font-size:16px;}
