/* 登录页面 */
.con_w{height:800px;width:100%;}
.log_tit{text-align:center;}
.log_tit h2{font-size:36px;color:#393939;font-weight:normal;line-height:58px;}
.log_tit h2 span{font-size:20px;color:#393939;}
.log_tit p{line-height:42px;color:#818181;font-size:16px;}
.log_con{width:363px;background-color:#f7f7f7;margin:0 auto;padding-bottom:33px;box-shadow:0px 2px 5px #ccc;
-moz-box-shadow:0px 2px 5px #ccc;
-webkit-box-shadow:0px 2px 5px #ccc;}
.log_con .touxiang{width:102px;height:102px;padding:37px 0 27px 0;margin:0 auto;text-align:center;}
.log_con .log_inps{width:283px;margin:0 auto;}
.log_con .log_inps p input{width:281px;height:43px;border:1px solid #e4e4e4;border-top:1px solid #d0d0d0;display:block;width:100%;outline:none;padding-left:10px;margin-top:11px;font-size:18px;color:#404040;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.log_con .log_inps p:nth-child(1) input{margin-top:0px;}
.log_con .log_inps p.forget_pass{text-align:right;margin-top:8px;}
.log_con .log_inps p.forget_pass a{font-size:14px;color:#f25a0b;}
.log_con .log_inps p .LoginBtn{background-color:#f25a0b;border:1px solid #ce4701;border-radius:3px;color:#fff;font-size:18px;margin-top:20px;cursor:pointer;}
.to_reg{text-align:center;}
.to_reg a{font-size:14px;color:#f25a0b;line-height:59px;}

/* 找回密码 */
.log_con .fi_lock{width:66px;height:66px;}

/* 注册成功 */
.reg_done_w{width:590px;margin:0 auto;text-align:center;margin-top:70px;height:690px;}
.reg_done_w h2{font-size:36px;color:#f25a0b;}
.reg_done_w h3.hint{padding-left:35px;background:url(/img/hint.png) no-repeat left center;width:42px;margin:0 auto;text-align:center;font-size:14px;color:#f25a0b;height:34px;line-height:34px;margin-top:50px;}
.reg_done_w h3{margin-top:40px;font-size:24px;color:#616161;font-weight:normal;}
.reg_done_w p{font-size:14px;color:#616161;margin-top:10px;line-height:30px;}
.reg_done_w p span{color:#e14831;}
.reg_done_w p.back{font-size:18px;color:#616161;line-height:37px;}
.reg_done_w p.back a{color:#f25a0b;}