	<script type="text/javascript">
		var timer;
		$('.check_hover').hover(function() {
			$('.hover_content').show();
			clearTimeout(timer);
		}, function() {
			timer = setTimeout(function(){
				$('.hover_content').hide();
			},200);
		});
		$('.hover_content').hover(function() {
			clearTimeout(timer);
		}, function() {
			timer = setTimeout(function(){
				$('.hover_content').hide();
			},200);
		});

		// 模态窗口
		$('.refuse').click(function(){
			$('.mo_del').show();
		});
		$('.model_content span').click(function(){
			$('.mo_del').hide();
		});
	</script>
