.car-detail{min-height:635px;}
.detail-photos p{cursor:pointer;position:relative;}
.detail-photos p input.file-add{width:44px;height:44px;position:absolute;top:1px;left:-2px;z-index:10;background:url(../img/car_add.png) no-repeat;}
.detail-photos p .p-add{position:absolute;top:90px;left:0px;z-index:10;}
.detail-photos input.file-upload{display:none;}
.detail-photos p .picture{width:100%;height:100%;border-radius:4px;position:absolute;top:0;left:0;}
.useline:before{content:"至";display:block;color:#999;padding:2px 5px;background:#fff;position:absolute;left:-11px;top:50px;z-index:99;font-weight:normal;}
.useline:after{content:"";display:block;width:10px;height:52px;border:1px solid #e2e2e2;border-right:none;position:absolute;top:35px;left:1px;}

/* 下拉框样式 */
.selectDark.ui-multiselect.ui-widget{width:355px!important;}
.short_sel .selectDark.ui-multiselect.ui-widget{width:170px !important;}
.short_sel{padding-left:15px;}
.up_top{margin-top:-5px;}
.mar_top{margin-top:20px;}