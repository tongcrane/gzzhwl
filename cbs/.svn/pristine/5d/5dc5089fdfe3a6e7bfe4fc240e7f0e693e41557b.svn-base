	padding:0 20px;
}
.per_detail>h3,.edit_pass>h3{
	font-size: 20px;
    padding-bottom: 15px;
    border-bottom: 1px solid #dcdcdc;
    margin-bottom: 15px;
    color: #525252;
    margin:17px 0 40px 0;
    font-weight: normal;
}
    
.message,.password{
	height:160px;
}
.message img,.password img{
	width:96px;
	height:96px;
	border-radius: 50%;
	overflow:hidden;
	margin-left:10px;
}
.message img{margin-left:0;}
.mess_con{
	width:450px;
	margin-right:-20px;
}
.pass_con{
	width:396px;
}
.submit,.pass_sub{
	position:relative;
}
.submit a,.pass_sub a{
	height:28px;
	line-height: 28px;
	font-size:14px;
	color:#444;
    background: #de642c;
    border: 1px solid #bc5120;
	cursor: pointer;
	padding:0 30px;
	position:absolute;
	top:14px;
	right:20px;
	color:#fff;
	border-radius: 2px;
}
.per_detail span,.edit_pass span{
	width: 20px;
    height: 20px;
    background: url(/img/adv_sea_close.png) no-repeat center;
    position: absolute;
    top: 8px;
    right: 8px;
    cursor: pointer;
}
.mess_div{
	display: inline-block;
	margin:0 5px 30px 0;
}
.mess_div label,.pass_div label{
	font-size:12px;
	color:#737373;
}
.pass_div{
	height:56px;
}
.pass_div>b{
	display: block;
	margin-left:86px !important;
	margin-top:4px;
}
.pass_div label{
	display: inline-block;
	width:80px;
	text-align:left;
	font-size:14px;
}
.mess_div input,.pass_div input{
	font-size: 12px;
