/* 信息列表页面通用样式 */

#header{
	width:100%;
	min-width:1200px;
	height:59px;
	background-color: #f6f6f6;
	border-bottom:1px solid #eee;
	/* position:fixed;
	z-index: 9999; */
	
}
.header-content{
	width:1200px;
	height:59px;
	margin:0 auto;
	position:relative;
}
.header-content h2{
	width:100%;
	height:59px;
	line-height: 59px;
	font-size:24px;
	text-align:center;
	color:#262524;
	background: url(/img/header_logo.png) no-repeat 26px center;
}
.header-icon{
	height:60px;
	position:absolute;
	right:0;
	top:0;
}
.header-contents,.header-message,.header-users{
	display: block;
	height:60px;
	width:32px;
	float:left;
	margin-right:10px;
	cursor: pointer;	
}
.header-contents{
	background: url(/img/header_content.png) no-repeat center;
	position:relative;
	
}
.header-message{
	background: url(/img/header_message.png) no-repeat center;
}
.header-users{
	background: url(/img/head_user.png) no-repeat center;
	margin-right:26px;
}
.content-detail{
	width:240px;
	/*height:346px;*/
	background:#fff;
	position:absolute;
	top:48px;
	right:-44px;
	box-shadow: 0 0 8px #ccc;
	padding:0 16px 30px 16px;
	display: none;
	z-index: 99;
}
.content-detail:before{
	content:"";
	display: block;
	width:0;
	height:0;
	border:10px solid transparent;
	border-bottom-color: #fff;
	position:absolute;
	top:-17px;
	right:50px;
}
.content-detail li{
	width:120px;
	height:72px;
	float:left;
	margin-top:30px;
	position:relative;
}
.content-detail span{
	display: block;
	width:48px;
	height:48px;
	margin:0 auto;
   
   background-repeat: no-repeat;
    /*background-image: ;*/
    /*background-position: ;*/
}

.content-detail a{
	display: block;
	width:120px;
	height:24px;
	padding-top:48px;
	text-align:center;
	font-size:14px;
	color:#646464;
	position:absolute;
	top:0;
	left:0;
}
.hea_business{
	background: url(/img/hea_business.fw.png) no-repeat 0 0;
}
.hea_record{
	background:#fff url(/img/hea_chart.fw.png) no-repeat 0 0;
}
.hea_service{
	background:#fff url(/img/hea_flag.fw.png) no-repeat 0 0;
}
.hea_finance{
	background:#fff url(/img/hea_flash.fw.png) no-repeat 0 0;
}
.hea_Platform{
	background:#fff url(/img/hea_favorite.fw.png) no-repeat 0 0;
}
.hea_organ{
	background:#fff url(/img/hea-cloud.fw.png) no-repeat 0 0;
}
.header-contents:hover .content-detail{
	display: block;
}
.header-users:hover .person{
	display: block;
}
.content-detail .current{
	background-position:0 -48px;
}
/* 个人详情样式 */
.person{
	width:380px;
	height:200px;
	background:#fff;
	position:absolute;
	top:53px;
	right:4px;
	box-shadow: 0 0 8px #ccc;
	display: none;
	z-index: 99;
	cursor: default;
}
.person:before{
	content:"";
	display: block;
	width:0;
	height:0;
	border:10px solid transparent;
	border-bottom-color: #fff;
	position:absolute;
	top:-17px;
	right:28px;
}
.detail{
	height:130px;
	padding-top:20px;
}
.detail img{
	width:96px;
	height:96px;
	border-radius: 50%;
	overflow:hidden;
	margin-left:20px;
}
.produce{
	width:240px;
}
.produce h4{
	font-size:12px;
	font-weight: normal;
	color:#000;
}
.produce h4 b{
	font-weight: normal;
}
.produce p{
	font-size:12px;
	color:#666;
	margin:5px 0;
}
.produce a{
	font-size:12px;
	color:#3f67c5;
	display: block;
	margin-bottom:12px;
}
.produce button{
	height:31px;
	line-height: 30px;
	font-size: 13px;
	color:#fff;
	background:#d55c25;
	border:1px solid #c54f2b;
	padding:0 12px;
	cursor: pointer;
	outline: none;
}
.exit{
	height:50px;
	background:#f5f5f5;
	position: relative;
}
.exit a{
	height:31px;
	line-height: 30px;
	font-size:12px;
	color:#444;
	border:1px solid #e2e2e2;
	background:#fdfdfd;
	cursor: pointer;
	padding:0 12px;
	position:absolute;
	top:10px;
	right:20px;
}
/* 个人详情查看修改样式 */
.per_model{
	width:100%;
	height:100%;
	position:fixed;
	top:0;
	left:0;
	background: rgba(0,0,0,0.5);
	z-index: 999;

}
.per_detail,.edit_pass{
	width:560px;
	height:340px;
	background: #fff;
	box-shadow: 0 0 20px #666;
