.check .active{ background-color: #de642c;color:#fff;border: 1px solid #bc5120; border-radius: 2px;}
.check a{width:90px;}

.feedback{margin-bottom: 40px;} 
.feedback h3 {width: 1123px;height: 48px;line-height: 48px;padding-left: 25px;margin-bottom: 24px;border-bottom: 1px solid #e1e1e1;background: url(../img/detail_info.png) no-repeat 0 center;font-size: 16px;color: #525252;}

.layout .labe_l{display: inline-block;width: auto;margin-right:10px;}
.layout .inpu_t{width:192px;height:24px;}
.feedline .layout{margin-right:30px;}
.feedline .right_short{margin-right:20px;}
.feedline .right_none{margin-right:0;cursor: pointer;}
.right_short .inpu_t{width:414px;}
.right_none{width:23px;height:24px;background:#eee;border:1px solid #d7d7d7;position:relative;}
.right_none:before{content: '';position:absolute;top:12px;left:6px;width:12px;border-top:2px solid #7b7b7b;}
/* .layout .selec_t{width:230px;margin-left:4px;}*/

.btn a {float: right;width: 82px;text-align: center;height: 28px;line-height: 28px;border: 1px solid #dcdcdc;margin: 12px 32px 0 0;border-radius: 2px;font-size: 14px;color: #4c4c4c;background: #eee;}
.btn .add{position: relative;}
.btn .add:before{content: '';position:absolute;top:14px;left:34px;width:12px;border-top:2px solid #7b7b7b;}
.btn .add:after{content: '';position:absolute;top:9px;left:39px;height:13px;border-left:2px solid #7b7b7b;}
.btn .submit{margin:12px 0 0 0;}


.mar_top{margin-top:6px;}
.middle .mar_top,.right .mar_top{margin-top:34px;}
.middle .id_num,.right .id_num{margin-top:49px;}

/* 模态窗口 */
.model_content,.model_content_next{width:690px;height:366px;margin:-203px 0 0 -365px;padding:20px;}
.left.short,.right.short{width:340px;}
.model_content .layout .stage,.model_content .layout .labe_l,.model_content_next .layout .stage,.model_content_next .layout .labe_l{width:94px;}
.model_content .layout .spa_n,.model_content .layout .inpu_t,.model_content_next .layout .spa_n,.model_content_next .layout .inpu_t{width:244px;}
.model_content .layout_two .inpu_t{width:160px;}
.model_content .layout_two .mar_rig{margin-right:12px;}
.model_content h4,.model_content_next h4{font-size:20px;padding-bottom:15px;border-bottom:1px solid #dcdcdc;margin-bottom: 15px;color:#525252;}
.lab_line{position: relative;}
.lab_line:after{content:"" ;width:8px;height:3px;border-bottom:1px solid #dcdcdc;display: block;position: absolute;top:11px;left:260px;}
.model_content_next,.model_content_cansel{
	background: #fff;
	position: fixed;
	top:50%;
	left:50%;
	box-shadow: 0 1px 8px #ccc;
	display: none;
}
.model_content textarea{
	display: block;
	width:372px;
	height:76px;
	border:1px solid #dedede;
	border-radius: 4px;
	margin:52px auto 0;
	text-indent: 6px;
	padding-top:4px;
	color:#525252;
	resize: none;
	outline:none;
}
.model_content a,.model_content_next a,.model_content_cansel a{
	height:28px;
	line-height: 28px;
	background: #de642c;
	border: 1px solid #bc5120;
	padding:0 30px;
	border-radius: 2px;
	color:#fff;
	position:absolute;
	right:20px;
	bottom:32px;
}
.model_content i,.model_content_next i,.model_content_cansel i{
	background:url("/img/adv_sea_close.png") no-repeat center;
    cursor: pointer;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 5px;
    top: 5px;
}

/* 订单取消弹框 */
.model_content_cansel{width:414px;height:200px;margin:-100px 0 0 -212px;}
a.mo_con_no{right:108px;}
a.mo_con_yes{right:214px;}
.model_content_cansel p{padding:40px;color:#525252;text-align: center;margin-top: 20px;}

