.detail-photos .down{
	margin-top:50px;
}
.detail-photos p{
	cursor:pointer;
	position:relative;
}

.detail-photos p input.file-add{
	width:44px;
	height:44px;
	position:absolute;
	top:1px;
	left:-2px;
	z-index: 10;
	background:url(/img/car_add.png) no-repeat;
}
.detail-photos p .p-add{
	position:absolute;
	top:90px;
	left:0px;
	z-index: 10;
}
.detail-photos input.file-upload{
	display:none;
}
.detail-photos p .picture{
	width:100%;
	height:100%;
	border-radius: 4px;
	position:absolute;
	top:0;
	left:0;
}
.f-tips .up-need {
    background: url(../img/star.png) no-repeat 66px center;
}
