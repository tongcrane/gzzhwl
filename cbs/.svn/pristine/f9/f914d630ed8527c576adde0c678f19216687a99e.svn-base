.detail-info .re-mark{
	width:170px;
	font-size:14px;
	float: right;
	color:#6e6e6e;
	display: block;
}
#show-pic{
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.6);
	position:fixed;
	top:0;
	left:0;
	z-index: 9999;
	display:none;
}
#show-pic .content{
	width:700px;
	height:700px;
	background: #fff;
	border-radius:10px;
	position:absolute;
	top:50%;
	left:50%;
	margin-top:-350px;
	margin-left:-350px;
}
#show-pic .content-img{
	width:100%;
	height:100%;
	background-position: center;
    background-repeat: no-repeat;
}
#show-pic .content span{
	width:18px;
	height:18px;
	background: url(../img/show_pic_bg.png) no-repeat;
	position:absolute;
	top:10px;
	right:10px;
	cursor:pointer;
}
.detail-info p label{
	width:130px;
}
.detail-info p .span-right{
	width:170px;
}
