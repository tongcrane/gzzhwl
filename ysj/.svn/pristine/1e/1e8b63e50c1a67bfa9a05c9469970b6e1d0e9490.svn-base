/* 登录页面 */
.con_w{height:770px;width:100%;}
.log_tit{text-align:center;}
.log_tit h2{font-size:36px;color:#393939;font-weight:normal;line-height:26px;margin-top:34px;}
.log_tit p{line-height:60px;color:#818181;font-size:20px;color:#292929;}
.log_con{width:363px;background-color:#f7f7f7;margin:0 auto;padding-bottom:33px;box-shadow:0px 2px 5px #ccc;
-moz-box-shadow:0px 2px 5px #ccc;
-webkit-box-shadow:0px 2px 5px #ccc;}
.log_con .touxiang{width:102px;height:102px;padding:37px 0 27px 0;margin:0 auto;text-align:center;}
.log_con .log_inps{width:283px;margin:0 auto;}
.log_con .log_inps p input{width:281px;height:43px;border:1px solid #e4e4e4;border-top:1px solid #d0d0d0;display:block;width:100%;outline:none;padding-left:10px;margin-top:11px;font-size:16px;color:#404040;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.log_con .log_inps p input:focus{border:1px solid #2b84fb;}
.log_con .log_inps p:nth-child(1) input{margin-top:0px;}
.log_con .log_inps p.forget_pass{text-align:right;margin-top:8px;}
.log_con .log_inps p.forget_pass a{color:#f25a0b;font-size:14px;}
.log_con .log_inps p.forget_pass a:hover{color:#f25a0b;}
.log_con .log_inps p .LoginBtn{background-color:#f25a0b;border:1px solid #ce4701;border-radius:3px;color:#fff;font-size:18px;margin-top:11px;cursor:pointer;}
.log_con .log_inps p .LoginBtn:hover{background-color:#e14d00;outline:none;}
.log_con .log_inps p .LoginBtn.disabled{background-color:#eee;border:1px solid #dcdcdc;color:#727272;outline:none;}
.log_con .log_inps p .LoginBtn:focus{border:none;}
.to_reg{text-align:center;}
.to_reg a{font-size:14px;color:#f25a0b;line-height:59px;}

/* 找回密码 */
.log_con .fi_lock{width:66px;height:66px;}

/* 注册成功 */
.reg_done_w{width:590px;margin:0 auto;text-align:center;margin-top:70px;height:690px;}
.reg_done_w h2{font-size:36px;color:#f25a0b;}
.reg_done_w h3.hint{padding-left:35px;background:url(../img/hint.png) no-repeat left center;width:42px;margin:0 auto;text-align:center;font-size:14px;color:#f25a0b;height:34px;line-height:34px;margin-top:50px;}
.reg_done_w h3{margin-top:40px;font-size:24px;color:#616161;font-weight:normal;}
.reg_done_w p{font-size:14px;color:#616161;margin-top:10px;line-height:30px;}
.reg_done_w p span{color:#e14831;}
.reg_done_w p.back{font-size:18px;color:#616161;line-height:37px;}
.reg_done_w p.back a{color:#f25a0b;}

/* 登录对话框 */
.login_w{width:480px;height:330px;}
.login_w form{margin:153px 0 0 82px;width:316px;}
.login_w input,.login_w button{width:100%;height:34px;border:1px solid #dfdee3;border-radius:6px;line-height:34px;}
.login_w input{padding-left:20px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;font-size:14px;}
.login_w input:nth-child(1){margin-bottom:16px;}
.login_w input:nth-child(2){margin-bottom:26px;}
.login_w button{background-color:#ff6037;font-size:18px;color:#fff;border:1px solid #ff6037;-o-transition:background .5s;-ms-transition:background .5s;-moz-transition:background .5s;-webkit-transition:background .5s;}
.login_w button:hover{background-color:#ff4814;-o-transition:background .5s;-ms-transition:background .5s;-moz-transition:background .5s;-webkit-transition:background .5s;}

/* 登录2.0 */
.login_wrap{/* height:789px; */background:url(../img/login_bg.jpg) no-repeat center;-webkit-background-size:cover;
background-size:cover;}
.con_foot.mt0{margin-top:0px;display:block;}
.login_wrap .login_inner{height:100%;margin:0 auto;position:relative;}
.login_wrap .login_inner .dialog_w{position:absolute;top:50%;left:50%;background-color:rgba(255,255,255,.6);width:360px;height:256px;-webkit-transform:translate(-50%,-50%);
-ms-transform:translate(-50%,-50%);
-o-transform:translate(-50%,-50%);
transform:translate(-50%,-50%);}
.login_wrap .login_inner .dialog_w .error{font-size:12px;color:#333;line-height:36px;text-align:center;display:block;}
.login_wrap .login_w input{padding:0 31px 0 20px;background-color:#fafdf8;}
.login_wrap .login_w input.clear{color:#999;}
.login_wrap .login_w input::-ms-clear{display:none;}
.login_wrap .login_w form{width:286px;margin:22px auto 0;}
.login_wrap .login_w form p{position:relative;}
.login_wrap .login_w form span.title{font-size:18px;color:#333;display:block;margin-bottom:20px;}
.login_wrap .login_w form p em.close{display:block;width:14px;height:14px;background:url(../img/close_pic.png) no-repeat center;position:absolute;right:17px;top:10px;cursor:pointer;}
.login_wrap{height:calc(100% - 355px);height:-moz-calc(100% - 355px);height:-webkit-calc(100% - 355px);height:-ms-calc(100% - 355px);height:-o-calc(100% - 355px);margin-bottom:187px;}
.login_wrap .login_w form button{margin-top:10px;}