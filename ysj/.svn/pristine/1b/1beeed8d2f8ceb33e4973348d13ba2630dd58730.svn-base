					<dl class="clearfix">
						<dt>报价</dt>
						<dd>
							<i ng-bind="info.price"></i>元
						</dd>
					</dl>
					<dl class="clearfix">
						<dt>留言</dt>
						<dd ng-bind="info.remark"></dd>
					</dl>
					<button class="btn cancelPrice" ng-if="info.status =='01'" ng-click="showCPDiag()">取消报价</button>
