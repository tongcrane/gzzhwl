/* 内部头部样式 */
.con_header{width:100%;height:44px;box-shadow: 0px 3px 3px #ccc;position:fixed;top:0px;left:0px;background-color:#fff;z-index:9999;}
.con_header .wrap{padding:7px 0;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.con_header .logo{float:left;margin:0 26px 0 25px;}
.con_header .h_line{float:left;width:1px;height:29px;background-color:#dcdcdc;}
.con_header .nav{float:left;margin-left:14px;}
.con_header .nav li{float:left;height:30px;line-height:30px;}
.con_header .nav li a{padding:0 11px;font-size:18px;color:#363636;}
.con_header .nav li a:hover{font-weight:bold;}
.con_header .search_w{float:right;}
.con_header .search_w .search_c{width:243px;height:27px;border:1px solid #1c1c1c;float:left;}
.con_header .search_w .search_c input{float:left;height:26px;line-height:26px;width:210px;outline:none;border:0;}
.con_header .search_w .search_c .serch_icon{float:right;display:block;background:url(../img/search.png) no-repeat center;height:100%;width:15px;margin-right:8px;cursor:pointer;}
.con_header .search_w .notice{width:17px;margin-left:16px;float:left;height:29px;background:url(../img/notice.png) no-repeat center;cursor:pointer;}
.con_header .search_w .user{float:left;}
.con_header .search_w .user a{display:block;width:32px;height:32px;background:url(../img/user.png) no-repeat center;margin:0 18px 0 20px;margin-top:-2px;}
.index_con_header{box-shadow:none;}
/* 内部底部样式 */
.con_foot p{line-height:22px;font-size:12px;color:#969696;margin-left:640px;}
.con_foot p span{margin-left:325px;}

/* 信息录入经纪人对话框 */
.content_w .con_a .c_tit{padding-left:38px;background:url(../img/agent_bg.png) no-repeat 10px center;}
.content_w .con_a p.txt_a{font-size:14px;color:#999;line-height:29px;line-height:29px;text-align:center;border:none;width:226px;}
.content_w .con_a .agent_w{position:relative;}
.content_w .con_a .agent_w .huo{display:block;width:23px;height:66px;background:url(../img/huo.png) no-repeat center;position:absolute;top:10px;left:-28px;}

.dialog_w .upload_c{width:730px;margin:55px auto 0;}
.dialog_w .upload_c .upload_c_l{float:left;width:339px;height:253px;border:1px solid #dfd9db;border-radius:5px;padding:12px 14px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.dialog_w .upload_c .upload_c_r{float:right;cursor:pointer;position:relative;}
.dialog_w .upload_c .upload_c_l img{width:100%;height:100%;}
.dialog_w .upload_c .upload_c_r span{display:block;width:86px;height:86px;background:url(../img/upload_b.png) no-repeat center;position:absolute;top:204px;left:279px;}
.dialog_w .upload_c p{font-size:14px;color:#999;line-height:38px;margin-top:10px;text-align:center;margin-bottom:125px;}
/* 加载中 */
.load-container{height:100%;overflow:hidden;-moz-box-sizing:border-box;box-sizing:border-box;}
.loader{-webkit-transform:translateZ(0);-moz-transform:translateZ(0);-ms-transform:translateZ(0);-o-transform:translateZ(0);transform:translateZ(0);}
.load-container .loader{margin:3.5em auto;font-size:10px;position:relative;text-indent:-9999em;border-top:1.4em solid #fddb3b;border-right:1.4em solid #fddb3b;border-bottom:1.4em solid #5d6d7e;border-left:1.4em solid #5d6d7e;-webkit-animation:load-container 1.3s infinite linear;animation:load-container 1.3s infinite linear;}
.load-container .loader,.load-container .loader:after{border-radius:50%;width:10em;height:10em;}
@-webkit-keyframes load-container{
	0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
	100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}
}
@keyframes load-container{
	0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
	100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}
}

/* 注册完成对话框 */
.dialog_done .star{text-align:center;}
.dialog_done .star img{width:98px;height:98px;}
.dialog_done .txt{text-align:center;margin-top:30px;}
.dialog_done .txt_we{margin-top:15px;}
.dialog_done .txt_dr a{font-size:18px;color:#f25a0b;}
.dialog_done .txt .txt_d{font-size:36px;color:#484848;}
.dialog_done .txt .txt_w{font-size:24px;color:#616161;}
.dialog_done .txt_p{font-size:14px;color:#7d7d7d;line-height:23px;}
.dialog_done .txt_a{text-align:right;margin:58px 90px 46px 0;}
.dialog_done .txt_a a{font-size:18px;color:#f25a0b;padding-left:49px;background:url(../img/info.png) no-repeat left center;display:inline-block;height:30px;line-height:30px;}
.dialog_done .txt_a a:hover{text-decoration:underline;color:#f25a0b;}
.dialog_done .txt_dr{font-size:14px;color:#7d7d7d;}
.dialog_done .txt_dr a{font-size:14px;}
/* dialog公共样式 */
.ui-widget-header{border:none;background:#fff;}
.ui-state-default .ui-icon{background:#fff url(../img/dio_close.png);border:none;}
.ui-state-default .ui-icon:hover{background-image:url(../img/dio_close_active.png);}
.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default{background-color:#fff;border:none;outline:none;}
.ui-icon-closethick{background-position:0px 0px;}
.ui-widget-content{border:none;border-radius:6px;}
.ui-widget-overlay{opacity:0.5;background-color:#000;z-index:10000;}
.ui-dialog .ui-dialog-content{background-color:#fff;}
.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default{background:none;}