.dialog_w{width:848px;height:530px;background-color:#fff;border-radius:6px;position:fixed;left:50%;top:50%;transform:translate(-50%,-50%);z-index:10003;box-shadow:2px 2px 5px #888888;}
.dialog_w .upload_c{width:730px;margin:98px auto 0;}
.dialog_w .upload_c .upload_c_l{float:left;width:339px;height:253px;border:1px solid #dfd9db;border-radius:5px;padding:12px 14px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.dialog_w .upload_c .upload_c_r{float:right;cursor:pointer;position:relative;}
.dialog_w .upload_c .upload_c_l img{width:100%;height:100%;}
.dialog_w .upload_c .upload_c_r span{display:block;width:86px;height:86px;background:url(../img/upload_b.png) no-repeat center;position:absolute;top:204px;left:279px;}
