-moz-box-sizing: border-box;
box-sizing: border-box;} */
.reg_con .reg_r .code_w{position:relative;}
.reg_con .reg_r span.picCode{position:absolute;top:0px;right:8px;cursor:pointer;}
.reg_con .reg_r span img{width:91px;height:32px;margin-top:3px;}
.reg_con .reg_r .reg_w div span.add{width:36px; font-size:14px;color:#f25a0b;font-weight:bold;display:inline-block;height:33px;line-height:33px;cursor:pointer;}
.reg_con .reg_r .reg_w div span.most{font-size:14px;color:#adadad;display:inline-block;height:33px;line-height:33px;margin-left:-8px;}
.reg_con .reg_r ul.route_w li{width:100%;position:relative;height:40px;line-height:40px;border:1px solid #dbdbdb;margin-top:26px;padding-left:10px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.reg_con .reg_r .reg_w div{position:relative;}
.reg_con .reg_r .reg_w div .msg{display:block;width:334px;min-height:65px;position:absolute;top:25px;left:-353px;background-color:#fff;border:1px solid #b8b8b8;box-shadow:0px 1px 3px #ccc;line-height:20px;padding:8px 14px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;color:#3b3b3b;font-size:14px;}
.reg_con .reg_r .reg_w div .msg .triangle{display:block;width:0px;height:0px;border:16px solid #bcbcbc;border-top:16px solid transparent; border-right:16px solid transparent;border-bottom:16px solid transparent;position:absolute;top:13px;left:333px;}
.reg_con .reg_r .reg_w div .msg .triangle:before{content:"";display:block;width:0px;height:0px;border:16px solid #fff;border-top:16px solid transparent; border-right:16px solid transparent;border-bottom:16px solid transparent;position:absolute;top:-16px;left:-18px;}
.reg_con .reg_r ul.route_w li .from em.are{font-size:14px;color:#adadad;}
.reg_con .reg_r ul.route_w li span.from em.city{font-size:18px;color:#363636;margin-left:12px;}
.reg_con .reg_r ul.route_w li .to em.are{font-size:14px;color:#adadad;}
.reg_con .reg_r ul.route_w li span.to em.city{font-size:18px;color:#363636;margin-left:12px;}
.reg_con .reg_r ul.route_w li span.close{position:absolute;top:0px;right:10px;cursor:pointer;font-size:14px;color:#f10b0b;}
