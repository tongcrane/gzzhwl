.reg_hint{height:40px;}
.reg_tit{font-size:26px;color:#808080;margin-top:48px;margin-left:564px;}
.reg_con{width:1200px;background:url(../img/reg_bg.png) no-repeat left 40px;margin-bottom:42px;}
.reg_con .reg_r{float: right;width:494px;background-color:#f1f1f1;margin-top:30px;padding-bottom:42px;}
.reg_con .reg_r .reg_w{width:424px;margin:32px auto 0;}
.reg_con .reg_r .reg_w div.reg-row{margin-top:17px;}
.reg_con .reg_r .reg_w div.add_w{margin-top:0px;}
.reg_con .reg_r label{display:block;line-height:38px;font-size:18px;color:#222222;}
.reg_con .reg_r input{width:100%;height:40px;border:1px solid #d9d9d9;border-top:1px solid #c1c1c1;padding-left:10px;font-size:16px;color:#404040;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.reg_con .reg_r div{position:relative;}
.reg_con .reg_r div .change_code{font-size:14px;color:#adadad;position:absolute;top:82px;right:0px;}
.reg_con .reg_r input:focus{border:1px solid #2b84fb;}
.reg_con .reg_r input.code{width:100%;float:left;padding-right:80px;}
.reg_con .reg_r .getMobileCode_w{position:relative;}
.reg_con .reg_r span.getMobileCode{position:absolute;top:12px;right:8px;font-size:14px;color:#f25a0b;cursor:pointer;}
.reg_con .reg_r span.getMobileCode.disabled{color:#adadad;}
.reg_con .reg_r input.getMobileCode{width:171px;float:right;font-size:14px;color:#fff;padding-left:0px;text-align:center;cursor:pointer;border-radius:4px;background-color:#f25a0b;border:1px solid #ce4701;}
.reg_con .reg_r input.getMobileCode.default{background-color:#eee;color:#727272;border:1px solid #dcdcdc;}
.reg_con .reg_r input.agree{width:13px;height:13px;border:1px solid #d9d9d9;margin-right:10px;}
.reg_con .reg_r .agree_w a{color:#f25a0b;}
.reg_con .reg_r .role{width:100%;height:40px;border:1px solid #d9d9d9;border-top:1px solid #c1c1c1;outline:none;}
.reg_con .reg_r .role option{width:211px;height:36px;line-height:36px;font-size:16px;color:#383838;}
.reg_con .reg_r label span{color:#f52b00;}
.reg_con .reg_r input.nextStep{width:131px;height:38px;background-color:#f25a0b;border:1px solid #ce4701;border-radius:3px;font-size:18px;color:#fff;text-align:center;line-height:38px;float:right;cursor:pointer;padding-left:0px;}
.reg_con .reg_r input.nextStep:hover{background-color:#e14d00;}
.reg_con .reg_r input.nextStep.disabled{background-color:#eee;border:1px solid #dcdcdc;color:#727272;}
