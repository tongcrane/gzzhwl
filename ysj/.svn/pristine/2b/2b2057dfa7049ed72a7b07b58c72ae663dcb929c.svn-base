/* 模拟表格 */
.table .tr .th,.table .tr .td{font-size:14px;color:#333;}
.table .tr .th{height:50px;line-height:50px;background-color:#f2f3f9;}
.table .tr .td{height:27px;line-height:27px;}
.table .tr .th,.table .tr .td{float:left;}
.table .tr .th:nth-child(1),.table .tr .td:nth-child(1){width:210px;text-align:left;}
.table .tr .th:nth-child(1){padding-left:48px;}
.table .tr .th:nth-child(2),.table .tr .td:nth-child(2){width:200px;text-align:left;}
.table .tr .th:nth-child(3),.table .tr .td:nth-child(3){width:200px;text-align:left;}
.table .tr .th:nth-child(4),.table .tr .td:nth-child(4){width:162px;text-align:right;}
.table .tr .th:nth-child(5),.table .tr .td:nth-child(5){width:150px;text-align:left;padding-left:119px;}
.table .tr .th:nth-child(6){width:109px;}
.table .tr .td:nth-child(6){width:64px;}
.table .tr .td:nth-child(1),.table .tr .td:nth-child(3){color:#999;}
.table .tr .td:nth-child(4){font-weight:bold;}
.table .tr .td:nth-child(4) p{padding-right:23px;background:url(../img/arrow_bg.png) no-repeat right center;cursor:pointer;position:relative;}
.table .tr_w{padding:44px 0 45px;width:1106px;margin:0 auto;border-bottom:1px solid #e8e8e8;}
.table .tr .td:nth-child(4) p span{display:block;padding:9px 49px 9px 19px;background-color:#fff;position:absolute;top:31px;right:-15px;text-align:left;border:1px solid #e8e8e8;z-index:99;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;border-radius:4px;}
.table .tr .td:nth-child(4) p span:before{content:"";display:block;width:0px;height:0px;border:8px solid #e8e8e8;border-left:8px solid transparent;border-right:8px solid transparent;border-top:8px solid transparent;position:absolute;top:-17px;right:13px;}
.table .tr .td:nth-child(4) p span:after{content:"";display:block;width:0px;height:0px;border:8px solid #fff;border-left:8px solid transparent;border-right:8px solid transparent;border-top:8px solid transparent;position:absolute;top:-16px;right:13px;}
.table .tr .td:nth-child(4) p span i{font-style:normal;display:block;white-space:nowrap;font-size:12px;color:#666;}
.table .tr .td:nth-child(4) p span i.font14{font-size:14px;color:#333;}
.table .tr .td:nth-child(6) .select_w{border:1px solid #d2d2d2;border-radius:3px;}
.table .tr .td:nth-child(6) .select a{color:#4d5aaf;text-decoration:none;padding-right:5px;float:left;padding-left:6px;}
.table .tr .td:nth-child(6) .select span{display:inline-block;background:url(../img/arrow_bg.png) no-repeat center;width:14px;height:8px;float:left;margin-top:10px;cursor:pointer;}
.table .tr .td:nth-child(6) .select_w .select span{padding-right:6px;}
.table .tr .td:nth-child(6) .select_w .select{border-bottom:1px solid #d2d2d2;}
.table .tr .td:nth-child(6) .select_w .select_down{padding-left:6px;background-color:#f2f3f9;color:#ff9400;cursor:pointer;}
.table .tr .td:nth-child(6) .select_down{display:none;}
.table .tr .td:nth-child(6) .select_w .select_down{display:block;}

