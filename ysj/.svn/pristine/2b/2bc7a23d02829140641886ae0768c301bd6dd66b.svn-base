/* 订单详情 */
.container{margin-top:62px;min-height:825px;}
.container .container_l{float:left;width:287px;}
.container .container_r{float:right;width:896px;}
.container .container_l a.flow_w:hover{text-decoration:none;}
.groom{width:100%;height:72px;background-color:#fff;line-height:72px;position:relative;box-shadow: 0px 1px 1px #ccc;}
.groom a.recommend_supply{display:block;height:72px;font-size:24px;color:#000;padding-left:47px;background:url(../img/groom_bg.png) no-repeat 19px center;}
.groom a:hover{text-decoration:none;}
.groom .arrow{display:block;width:8px;height:15px;background:url(../img/arrow.png) no-repeat center;position:absolute;right:16px;top:30px;cursor:pointer;}
.flow{box-shadow: 0px 1px 1px #ccc;width:100%;height:313px;background-color:#fff;margin-top:20px;padding:0 7px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;position:relative;}
.flow .flow_bar{height:57px;border-bottom:1px solid #d0d0d0;padding:20px 0 0 11px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.flow .flow_bar span{float:left;}
.flow .flow_bar span.city{font-size:24px;color:#000;}
.flow .flow_bar span.car_bg{width:114px;height:15px;background:url(../img/pic_car.png) no-repeat center;margin:5px 7px 0 15px;}
.flow dl.detail{height:164px;width:100%;border-bottom:1px solid #ccc;font-size:14px;color:#000;padding-top:7px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.flow dl.detail dt{float:left;width:100px;padding-left:10px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;line-height:25px;}
.flow dl.detail dd{float:left;line-height:25px;}
.flow dl.detail dd.w96{width:96px;}
.flow dl.detail dd.w160{width:160px;}
.flow dl.detail dd.w172{width:172px;}
.flow dl.detail dd.w76{width:76px;}
.flow dl.detail .font18{font-size:18px;}
.flow .name{font-size:14px;color:#337bf2;height:43px;line-height:43px;padding-left:39px;background:url(../img/man.png) no-repeat 16px center;}
.flow .count{position:absolute;bottom:0;left:1px;height:48px;line-height:48px;background-color:#fbfbfb;width:286px;}
.flow .count span{float:left;font-size:14px;color:#7a7a7a;width:142px;text-align:center;}
.flow .count span.line{width:1px;height:22px;background-color:#a0a0a0;margin-top:13px;}
.flow .count span i{font-style:normal;color:}
.flow .count span.days{font-size:14px;color:#ed6e00;}
.flow .count span.mans i{font-size:18px;color:#ed6e00;}
.container_r .line_detail{width:100%;background-color:#fff;box-shadow: 0px 1px 1px #ccc;}
.container_r .line_detail .tit{height:39px;line-height:39px;border-bottom:1px solid #eee;padding-left:27px;font-size:14px;color:#000;position:relative;}
.container_r .line_detail .tit .quoted_price{width:84px;height:29px;line-height:29px;text-align:center;font-size:14px;color:#a2a2a3;background-color:#eee;border-radius:3px;position:absolute;top:5px;right:10px;outline:none;}
.container_r .line_detail .content{padding:22px 45px 20px 25px;position:relative;}
.container_r .line_detail .content_l{float:left;}
.container_r .line_detail .content_r{float:right;margin-top:107px;}
.line_detail .content_l span{float:left;}
.line_detail .content_l span.city{font-size:24px;}
.line_detail .content_l span.font14{font-size:14px;color:#a0a0a0;margin:5px 9px 0 0;}
.line_detail .content_l span.font18{font-size:18px;margin-top:2px;}
.line_detail .content_l span.route_start{width:114px;height:15px;background:url(../img/route_start.png) no-repeat center;margin:3px 6px 0 17px;}
.line_detail .content_l span.route_end{width:85px;height:4px;background:url(../img/route_end.png) no-repeat center;margin:10px 7px 0 4px;}
.line_detail .content_l p.time{padding:24px 0 55px 0;}
.line_detail .content_l p.time span{font-size:14px;}
.line_detail .content_l p.time span.arrive_time{margin-left:70px;}
.line_detail .content_l div.car_detail{font-size:14px;height:50px;}
.line_detail .content_l .car_detail p.detail_l{float:left;width:73px;}
.line_detail .content_l p.detail_l span{display:block;width:100%;}
.line_detail .content_l p.detail_l span.font18{font-size:18px;}
.line_detail .content_l p.detail_l span.m5{margin-top:5px;}
.line_detail .content_l .car_detail p.w82{width:82px;}
.line_detail .content_l .car_detail p.w105{width:105px;}
.line_detail .content_l .car_detail p.w110{width:110px;}
/* .line_detail .content_l p.car_detail i{font-style:normal;font-size:18px;} */
.line_detail .content_l p .name{font-size:14px;color:#337bf2;margin-right:40px;padding-left:20px;background:url(../img/man.png) no-repeat left center;line-height:22px;}
.line_detail .content_l p .address{font-size:14px;line-height:22px;}
.line_detail .content_l p.addr_detail{padding:15px 0 22px 0;}
.line_detail .content_r p.font14{font-size:14px;color:#7a7a7a;margin-bottom:20px;text-align:center;}
.line_detail .content_r .font18{font-size:18px;color:#ed6e00;font-style:normal;text-align:center;}
.line_detail .content .tel{position:absolute;top:12px;right:-5px;width:190px;height:59px;background-color:#ef5b0a;color:#e2e1e1;text-align:center;}
.line_detail .content .tel span{display:block;}
.line_detail .content .tel .consult{font-size:14px;line-height:28px;}
.line_detail .content .tel .num{font-size:18px;color:#fff;line-height:28px;}
.container_r .quote_price{margin-top:19px;padding:0 0 24px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.quote_price .price_l{float:left;width:640px;font-size:14px;padding-left:25px;}
.quote_price .price_r{float:right;width:6px;height:28px;background:url(../img/close_price.png) no-repeat center;margin-top:47px;cursor:pointer;position:relative;margin-right:20px;}
.quote_price .price_l .remark_l{float:left;}
.quote_price .price_l .remark_r{float:left;width:573px;line-height:22px;}
.quote_price .price_l .doller{font-size:18px;color:#ef5b0a;background:url(../img/price_bg.png) no-repeat bottom center;padding:0px 20px;}
.quote_price .price_l .price{margin:20px 0 12px 0;}
.quote_price .price_r .close_price{font-size:14px;color:#ef843d;display:block;width:110px;height:45px;line-height:45px;text-align:center;background-color:#fff;border:1px solid #dadada;box-shadow:0px 2px 3px #ccc;position:absolute;top:33px;left:-53px;}
.container_r .hint{padding:20px 26px;}
.container_r .hint p{font-size:14px;line-height:22px;}
.container_r .hint p.more_rule{text-align:right;margin-top:16px;}
.container_r .hint p.more_rule a{color:#337bf2;text-decoration:underline;}
/* 无数据 */
.recommend .no-data{width:100%;height:314px;background-color:#fff;box-shadow: 0px 1px 1px #ccc;margin-top:20px;text-align:center;font-size:24px;color:#999;padding-top:42px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.recommend .no-data span.txt{padding-top:156px;background:url(../img/no_data.png) no-repeat center top;display:block;width:138px;margin:0 auto;}
.container .container_l .flow span.icon{width:120px;height:133px;background:url(../img/no_data01.png) no-repeat center bottom;display:block;margin:0 auto;padding-top:64px;}
.container .container_l .flow span.no_data{font-size:24px;color:#a0a0a0;display:block;text-align:center;margin-top:25px;}

/* 订单详情完善信息 */
.select_car_w dl.select_car{border-bottom:1px solid #eee;padding:17px 44px 15px 26px;font-size:14px;color:#000;}
.select_car_w dl.select_car span.no_data{font-size:24px;color:#999;display:block;text-align:center;margin-top:15px;padding-top:109px;background:url(../img/no_data02.png) no-repeat center top;width:120px;margin:0 auto;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
dl.select_car dt{float:left;width:87px;height:34px;line-height:34px;}
dl.select_car dd{float:left;}
dl.select_car dd.brands{width:692px;}
dl.select_car dd span{padding:0 19px;height:34px;line-height:34px;border:3px solid #eee;float:left;margin-right:10px;margin-bottom:11px;}
dl.select_car dd span.active{border:3px solid #ef5b0a;color:#000;}
dl.select_car dd span.disabled{border:3px solid #eee;color:#b2b2b2;}
dl.select_car dd.brands span:nth-child(6n){margin-right:0px;}
dl.select_car dd.name span:nth-child(6n){margin-right:10px;}
dl.select_car dd.add a{color:#337bf2;text-decoration:underline;}
dl.select_car dd.add{height:34px;line-height:34px;margin-left:19px;}
dl.select_car dd.txt button{float:left;width:83px;height:27px;line-height:27px;text-align:center;border-radius:2px;color:#575757;background-color:#eee;outline:none;}
dl.select_car dd.txt button.active{background-color:#ef5b0a;border:1px solid #cd5816;color:#fff;}
dl.select_car dd.txt p{float:left;height:27px;line-height:27px;margin-left:32px;padding-left:19px;background:url(../img/dot_bg.png) no-repeat left center;}

/* 订单详情资料已审过 */
.audit_pass dl.select_car dd.txt p{margin-left:0px;}
.container_r .auditpass_hint{padding:20px 26px 60px;}