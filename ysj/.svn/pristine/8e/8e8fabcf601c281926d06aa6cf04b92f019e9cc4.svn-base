.container_w .con .item p:nth-child(3) span.font18 em:nth-child(2){margin-left:10px;}
.container_w .con .item:nth-child(1){border-bottom:1px solid #e8e8e8;}
.log_info ul.track_w{padding:54px 0 0 54px;font-size:14px;color:#666;}
.log_info ul.track_w li{padding-bottom:75px;border-left:1px solid #acacac;position:relative;}
.log_info ul.track_w li:nth-child(1):before{content:"";display:block;width:1px;height:10px;background-color:#fff;position:absolute;top:0px;left:-1px;}
.log_info ul.track_w li span{display:inline-block;padding-left:29px;vertical-align:top;}
.log_info ul.track_w li span:nth-child(1){}
.log_info ul.track_w li span:nth-child(2){padding-left:41px;width:104px;}
.log_info ul.track_w li a{font-size:14px;color:#666;text-decoration:none;padding-left:35px;width:174px;display:inline-block;cursor:default;}
.log_info ul.track_w li span:nth-child(3){padding-left:46px;}
.log_info ul.track_w li span:nth-child(2).active{background-image:url(../img/dian_a.png);}
.log_info ul.track_w li:last-child{border-left:none;}
.log_info ul.track_w li:last-child:before{content:"";display:block;width:1px;height:10px;background-color:#acacac;position:absolute;top:0px;left:0px;}
.log_info ul.track_w li span.dot{width:11px;height:11px;background:url(../img/dian.png) no-repeat center;position:absolute;top:4px;left:-21px;}
.log_info ul.track_w li .abnormal{display:inline-block;vertical-align:top;padding-left:29px;}
.log_info ul.track_w li .abnormal p{padding-bottom:12px;}
.log_info ul.track_w li .abnormal p.reason{width:567px;}
.log_info ul.track_w li .abnormal p>em{padding-right:50px;float: left;height:19px;line-height:19px;}
.log_info ul.track_w li .abnormal p em.pr0{padding-right:0px;}
.log_info ul.track_w li .abnormal .img_w{margin-top:12px;}
.log_info ul.track_w li .abnormal .img_w em{width:96px;height:54px;background-color:#eee;margin-right:21px;display:inline-block;}
.log_info ul.track_w li .abnormal .img_w em:last-child{margin-right:0px;}
.log_info ul.track_w li .abnormal .img_w em img{width:96px;height:54px;}
.log_info ul.track_w li.active span.dot{background-image:url(../img/dian_a.png);}
.log_info ul.track_w li.active,.log_info ul.track_w li.active a{color:#ff6037;}
.log_info ul.track_w li.active a{text-decoration:underline;cursor:pointer;}
.log_info ul.track_w li .abnormal p em.warn{color:#ff6037;display:inline-block;width:130px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}

/*  地图详情2.0 */
.map_W{position:relative;}
.map_W .map_l{float: left;width:100%;height:800px;background-color:#eee;}
.map_W .map_r{float: right;padding:0 22px 0 16px;width:392px;-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}
.map_W .map_l .map{width:100%;height:100%;}
.map_W .map_r .top{height:79px;line-height:79px;border-bottom:1px solid #e8e8e8;}
.map_W .map_r .top_l{float: left;font-size:14px;color:#666;}
.map_W .map_r .top_r{float: right;font-size:12px;color:#666;}
.map_W .map_r .middle{padding:27px 0 23px 8px;border-bottom:1px solid #e8e8e8;}
.map_W .map_r .middle_l{float: left;width:64px;height:64px;background:url(../img/car_pic.png) no-repeat center;}
.map_W .map_r .middle_r{float: left;margin-left:20px;}
.map_W .map_r .middle_r p{font-size:14px;color:#666;}
.map_W .map_r .middle_r p.font18{font-size:18px;color:#333;padding:8px 0;}
.map_W .map_r .bottom{padding:37px 0 0 8px;}
.map_W .map_r .bottom p{font-size:14px;color:#666;line-height:29px;}
.map_W .normal{background-color:#fff;border-radius:3px;}
.map_W .normal .tit{height:47px;line-height:47px;font-size:12px;color:#666;border-bottom:1px solid #e8e8e8;width:350px;}
.map_W .normal .tit_l{float: left;}
.map_W .normal .tit_r{float: right;width:14px;height:14px;background:url(../img/tit_r.png) no-repeat center;cursor:pointer;margin-top:17px;}
.map_W .normal .con{padding-top:24px;}
.map_W .normal .con_l{float: left;width:64px;height:64px;background:url(../img/car_pic.png) no-repeat center;}
.map_W .normal .con_r{float: left;margin-left:35px;}
.map_W .normal .con_r span{font-size:12px;color:#666;display:block;}
.map_W .normal .con_r span:nth-child(1){font-size:18px;color:#333;margin-bottom:8px;}
.map_W .normal .con_r span:nth-child(2){margin-bottom:32px;}
.map_W .normal .con_r span:nth-child(3){margin-bottom:8px;}
.map_W .unnormal{top:400px;left:550px;}
.map_W .unnormal .con_r span:nth-child(2){margin-bottom:20px;}
.map_W .unnormal .con_r span:nth-child(3){margin-bottom:20px;font-size:14px;color:#333;}
.map_W .unnormal .con_r span:nth-child(3) em{display:inline-block;width:180px;vertical-align:top;}
.map_W .unnormal .con_r span:nth-child(4),.map_W .unnormal .con_r span:nth-child(5){margin-bottom:5px;}
.map_W .orgin{font-size:16px;color:#666;padding:10px 23px 5px 18px;top:100px;left:550px;line-height:33px;}
.map_W .orgin .close{width:14px;height:14px;background:url(../img/tit_r.png) no-repeat center;cursor:pointer;position:absolute;top:18px;right:18px;}
