    <body style="background-color:#fbfbfb;">
		<div class="con_header">
			<div class="wrap clearfix">
				<div class="logo">
					<a href="index.htm">
						<img src="/img/c_logo.png" height="32" width="184">
					</a>
				</div>
				<ul class="nav clearfix">
					<li>
						<a href="/index.htm">首页</a>
					</li>
					<li class="current">
						<a href="/supply_hall.htm">货源大厅</a>
					</li>
					<li>
						<a href="/my_order.htm">我的订单</a>
					</li>
				</ul>
				<div class="search_w">
					<span class="notice"></span>
					<span class="user">
						<a href="/center_basicinfo.htm"></a>
					</span>
				</div>
			</div>
		</div>
		<div class="order_tit">
			<div class="wrap">
				<h1>货源大厅</h1>
			</div>
		</div>
		<div class="order_list_w wrap">
			<div class="order_top clearfix">
				<div class="order_top_l">
					<ul class="clearfix tab_tit">
					</ul>
				</div>
				
				<div class="order_top_m">
					<a href="running_route.htm"><strong>进行添加</strong></a>
				</div>
				
				
				<div class="order_top_r clearfix">
					<input type="text" placeholder="货源编号/出发地/目的地"/>
					<button>搜索</button>
				</div>
			</div>
			<div class="order_filter">
				<ul class="filter_w clearfix">
					<li>
						<input type="text" placeholder="发布时间" class="pubdate" />
					</li>
					<li>
						<select>
							<option>出发地</option>
							<option>北京</option>
							<option>上海</option>
						</select>
					</li>
					<li>
						<select>
							<option>目的地</option>
							<option>北京</option>
							<option>上海</option>
						</select>
					</li>
					<li>
						<select>
							<option>车长</option>
							<option>1m</option>
							<option>2m</option>
						</select>
					</li>
					<li>
						<select>
							<option>车型</option>
							<option>高栏</option>
							<option>低栏</option>
						</select>
					</li>
					<li>
						<input type="radio"/>回程货源
					</li>
				</ul>
			</div>
			<ol class="order_list wrap">
				
			</ol>
		</div>
		<div class="list_page clearfix wrap">
			<div class="page_w">
				<div class="handle clearfix">
					<div class="handle-page clearfix"></div>					
				</div>
			</div>
		</div>
		<div class="recommend wrap">
			<div class="groom">
				<a href="supply_hall.htm">推荐货源</a>
				<a href="supply_hall.htm">
					<span class="arrow">更多</span>
				</a>
			</div>
			<ul class="recommend_list clearfix">
				<li>
					<a href="supply_detail.htm" class="flow_w">
						<div class="flow">
							<div class="flow_bar clearfix">
								<span class="city">广州</span>
								<span class="car_bg"></span>
								<span class="city">上海</span>
							</div>
							<dl class="detail clearfix">
								<dt>提货时间</dt>
								<dd>2012/12/12 12:00</dd>
								<dt>提货地点</dt>
								<dd>广东省 广州市</dd>
								<dt>需求车长</dt>
								<dd class="w96">货物吨位</dd>
								<dd>货物体积</dd>
								<dt class="font18">9.6米</dt>
								<dd class="font18 w96">8吨</dd>
								<dd class="font18">5方</dd>
								<dt>需求车型</dt>
								<dd class="w160">高栏</dd>
								<dt>货物类型</dt>
								<dd>电子配件</dd>
							</dl>
							<div class="name">广东志鸿物流有限公司</div>
							<div class="count clearfix">
								<span class="mans">已应标<i>26</i>人</span>
								<span class="line"></span>
								<span class="days">2天结束</span>
							</div>
						</div>
					</a>
				</li>
			</ul>
		</div>
		<div class="con_foot">
			<div class="wrap">
				<p>
					条款 - 隐私权
					<span>©2016 guangzhouzhihong</span>                                                 
				</p>
			</div>
		</div>
