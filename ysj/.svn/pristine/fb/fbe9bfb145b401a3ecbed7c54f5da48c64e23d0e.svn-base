.corporate_culture{height:660px;background-color:#f4f5f8;}
.corporate_culture .culture{width:1192px;margin:0 auto;text-align:center;}
.corporate_culture .culture .tit{font-size:24px;color:#333;position:relative;display:inline-block;margin-top:122px;}
.corporate_culture .culture .tit:before{content:"";display:block;width:150px;height:1px;background-color:#dfdee3;position:absolute;top:18px;left:-200px;}
.corporate_culture .culture .tit:after{content:"";display:block;width:150px;height:1px;background-color:#dfdee3;position:absolute;top:18px;left:150px;}
.corporate_culture .culture ul{margin-top:120px;}
.corporate_culture .culture ul li{float:left;padding-top:140px;background:url(../img/culture_bg_1.png) no-repeat top center;margin-right:60px;width: 250px;}
.corporate_culture .culture ul li:nth-child(2){background-image:url(../img/culture_bg_2.png);}
.corporate_culture .culture ul li:nth-child(3){background-image:url(../img/culture_bg_3.png);}
.corporate_culture .culture ul li:nth-child(4){background-image:url(../img/culture_bg_4.png);margin-right:0px;}
.corporate_culture .culture ul li span{display:block;text-align:center;font-size:28px;color:#333;}
.corporate_culture .culture ul li span:nth-child(2){font-size:14px;color:#666;margin-top:18px;line-height:20px;}


.history_w{background-color:#fff;}
.history{width:1192px;margin:0 auto;text-align:center;}
.history_w .history .tit{font-size:36px;color:#999999;position:relative;display:inline-block;margin-top:122px;margin-bottom: 60px;}
.history_w .history .pic{width:1192px;margin: 0 auto;}
.history_w .history .pic1{height:500px;background:url(../img/history_1.png) no-repeat center;}
.history_w .history .pic2{height:536px;background:url(../img/history_2.png) no-repeat center;}
.history_w .history .pic3{height:668px;background:url(../img/history_3.png) no-repeat center;}
.history_w .history .pic4{height:786px;background:url(../img/history_4.png) no-repeat center;}
.history_w .history .padding{height:220px;}