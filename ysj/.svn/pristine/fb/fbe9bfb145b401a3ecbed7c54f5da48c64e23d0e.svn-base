.banner_w{position:relative;height:450px;}
.banner_bg{width:1920px;height:450px;position:absolute;left:50%;margin-left:-960px;text-align:center;background:url(../img/about_bg.png) no-repeat center;}

.about_w{background-color:#fff;}
.about{width:1192px;margin:0 auto;text-align:center;}
.about_w .about .tit{font-size:24px;color:#333;position:relative;display:inline-block;margin-top:122px;}
.about_w .about .tit:before{content:"";display:block;width:150px;height:1px;background-color:#dfdee3;position:absolute;top:18px;left:-200px;}
.about_w .about .tit:after{content:"";display:block;width:150px;height:1px;background-color:#dfdee3;position:absolute;top:18px;left:150px;}
.about_w .about .text{width:816px;margin: 0 auto;text-align: left;line-height: 34px;color:#5e5e76;}
.about_w .about .pic{height:450px;background:url(../img/about_pic.png) no-repeat center;}
