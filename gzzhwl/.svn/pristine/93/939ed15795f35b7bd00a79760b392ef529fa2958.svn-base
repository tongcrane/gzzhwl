		<trim prefix="WHERE" prefixOverrides="AND |OR ">
		<if test="vehicleInfoId != null">
	  		and t2.vehicle_Info_Id = #{vehicleInfoId}
	  	</if>
		<if test="isDeleted != null">
	  		and t2.is_deleted = #{isDeleted}
	  	</if>
		<if test="accountId != null">
	  		and t3.account_id = #{accountId}
	  	</if>
		<if test="source != null">
	  		and t2.source = #{source}
	  	</if>
	  	<if test="driverList != null">
	  		and t1.driver_info_id in 
		  <foreach collection="driverList" index="index" item="driverId" open="(" separator="," close=")">  
		       #{driverId}
		  </foreach>
	  	</if>
		</trim>
	</select>
	
	
	<!-- 内部可使用车辆查询  -->
	<select id="getSysCanUserVehicleList" parameterType="map" resultMap="pageResultMap">
	   SELECT
		t1.vehicle_info_id,
		t1.plate_number,
		t1.`length`,
		t1.type,
		t1.brand,
		t1.color,
		t1.load_weight,
		t1.attributes,
		t3.departure_code,
		t3.destination_code,
		t1.bridge_type,
		t1.owner_name,
		t1.headstock_type,
		t3.belong_depart_id,
		t3.use_depart_id,
		zdib.name as belong_depart_name,
		zdiu.name as use_depart_name
		FROM
			zh_real_vehicle_info t1
		inner join zh_real_vehicle_plus_info t2
		on t1.vehicle_info_id = t2.vehicle_info_id
		inner join (zh_real_vehicle_used_info t3
			inner join zh_department_info zdib on zdib.depart_id = t3.belong_depart_id
			inner join zh_department_info zdiu on zdiu.depart_id = t3.use_depart_id
		)
		on t1.vehicle_info_id = t3.vehicle_info_id
		<trim prefix="WHERE" prefixOverrides="AND |OR ">
			<if test="source != null">
				and t1.source=#{source}
			</if>
			<if test="departId != null">
				and t3.use_depart_id = #{departId}
			</if>
			<if test="isDeleted != null">
				and t1.is_deleted=#{isDeleted}
			</if>
			<if test="vehicleType != null">
				and t1.vehicle_type = #{vehicleType}
			</if>
			<if test="notVehicleType != null">
				and t1.vehicle_type != #{notVehicleType}
			</if>
			<if test="useStatus != null">
				and	t3.use_status = #{useStatus}
			</if>
			<if test="arrVehicleIds != null">
				or t1.vehicle_info_id in 
				<foreach collection="arrVehicleIds" index="index" item="vehicleId" open="(" separator="," close=")">  
			           #{vehicleId}
			   	</foreach>
			</if>
		</trim>
	    order by t1.updated_time desc
	</select>
	
	<!--运势界可使用司机车辆列表查询  -->
	<select id="getUsableDriverAndVehicleList" parameterType="map" resultMap="UsableResultMap">
		SELECT
			distinct
			zrvdi.account_id,
			zrdi.vehicle_info_id,
			zrdi.plate_number,
			zrdi.type,
			zrdi.length
		FROM
			(
				SELECT
					vehicle_info_id,
					plate_number,
					length,
					type
				FROM
					zh_real_vehicle_info
				WHERE
					source = #{source}
			) zrdi
		INNER JOIN zh_real_vehicle_used_info zrvui ON zrdi.vehicle_info_id = zrvui.vehicle_info_id
		AND zrvui.use_status = #{useStatus}
		INNER JOIN (
			(
				SELECT
					*
				FROM
					zh_real_vehicle_driver_info
				WHERE
					account_id = #{accountId}
			) zrvdi
			INNER JOIN (
				zh_real_driver_info zrdi
				INNER JOIN zh_real_driver_used_info zrdui ON zrdi.driver_info_id = zrdui.driver_info_id
				AND zrdui.use_status = #{useStatus}
			) ON zrvdi.driver_info_id = zrdi.driver_info_id
		) ON zrdi.vehicle_info_id = zrvdi.vehicle_info_id
	</select>
	
</mapper> 