	<insert id="insert" parameterType="LoadArriveInfo">
	   insert into zh_load_arrive_info (
	     load_id,
	     arrive_time,
	     created_by,
	     created_time
	   )values (
	     #{loadId},
	     #{arriveTime},
	     #{createdBy},
	     now()
	   )
	</insert>

	<update id="update" parameterType="LoadArriveInfo">
